{
    "name": "brunocfalcao/twitter-copilot",
    "description": "Laravel Twitter Copilot - My way to publish scheduled tweets",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/brunocfalcao/twitter-copilot",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "brunocfalcao/laravel-helpers": "dev-master",
        "guzzlehttp/guzzle": "^7.8",
        "laravel-notification-channels/twitter": "^8.0",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Brunocfalcao\\TwitterCopilot\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brunocfalcao\\TwitterCopilot\\TwitterCopilotServiceProvider"
            ]
        }
    }
}
